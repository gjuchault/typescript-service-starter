{
  "name": "@gjuchault/typescript-service-starter",
  "version": "0.0.0-development",
  "description": "Yet another typescript service starter template",
  "keywords": [
    "typescript",
    "service",
    "starter",
    "template"
  ],
  "homepage": "https://github.com/gjuchault/typescript-service-starter",
  "bugs": "https://github.com/gjuchault/typescript-service-starter/issues",
  "author": "Gabriel Juchault <gabriel.juchault@gmail.com>",
  "repository": "gjuchault/typescript-service-starter",
  "main": "./build/cjs/index.js",
  "module": "./build/esm/index.js",
  "types": "./build/index.d.ts",
  "license": "MIT",
  "engines": {
    "node": "> 14"
  },
  "publishConfig": {
    "access": "public"
  },
  "volta": {
    "node": "16.15.0",
    "npm": "8.9.0"
  },
  "scripts": {
    "setup": "node -r ./esbuild-hook ./scripts/setup",
    "build": "npm run clean && npm run type:dts && npm run build:main",
    "build:main": "node -r ./esbuild-hook ./scripts/build",
    "build:client": "node -r ./esbuild-hook ./scripts/client",
    "start": "node ./build",
    "start:debug": "node --inspect ./build",
    "dev": "node -r ./esbuild-hook ./scripts/dev | pino-pretty",
    "clean": "node -r ./esbuild-hook ./scripts/clean",
    "migrate:up": "node -r ./esbuild-hook ./scripts/migrate up",
    "migrate:create": "node -r ./esbuild-hook ./scripts/migrate create",
    "type:dts": "tsc --emitDeclarationOnly",
    "type:check": "tsc --noEmit",
    "format": "prettier \"src/**/*.ts\" --write",
    "format:check": "prettier \"src/**/*.ts\" --check",
    "lint": "eslint src --ext .ts --fix",
    "lint:check": "eslint src --ext .ts",
    "test": "ava",
    "test:coverage": "nyc ava && nyc report --reporter=html",
    "test:setup": "node -r ./esbuild-hook ./scripts/testSetup",
    "spell:check": "cspell \"{README.md,CODE_OF_CONDUCT.md,CONTRIBUTING.md,.github/*.md,src/**/*.ts}\"",
    "cz": "cz",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/github": "^8.0.4",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/eslint": "^8.4.2",
    "@types/ms": "^0.7.31",
    "@types/node": "^17.0.33",
    "@types/prompts": "^2.0.14",
    "@types/sinon": "^10.0.11",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "ava": "^4.2.0",
    "camelcase": "^6.3.0",
    "commitizen": "^4.2.4",
    "cspell": "^5.20.0",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.14.3",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "estrella": "^1.4.1",
    "get-port": "^6.1.2",
    "json-schema-to-typescript": "^10.1.5",
    "json-schema-to-zod": "^0.0.0-beta.0",
    "launch-editor": "^2.3.0",
    "nyc": "^15.1.0",
    "pino-pretty": "^7.6.1",
    "prettier": "^2.6.2",
    "prompts": "^2.4.1",
    "semantic-release": "^19.0.2",
    "sinon": "^14.0.0",
    "slugify": "^1.6.5",
    "source-map-support": "^0.5.21",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@autotelic/fastify-opentelemetry": "^0.16.0",
    "@fastify/circuit-breaker": "^2.0.0",
    "@fastify/cookie": "^6.0.0",
    "@fastify/cors": "^7.0.0",
    "@fastify/csrf-protection": "fastify/csrf-protection@04d05fb",
    "@fastify/etag": "^3.0.0",
    "@fastify/formbody": "^6.0.1",
    "@fastify/helmet": "^8.0.1",
    "@fastify/multipart": "^6.0.0",
    "@fastify/rate-limit": "^6.0.0",
    "@fastify/swagger": "^6.0.1",
    "@opentelemetry/api": "^1.1.0",
    "@opentelemetry/api-metrics": "^0.28.0",
    "@opentelemetry/core": "^1.2.0",
    "@opentelemetry/exporter-prometheus": "^0.28.0",
    "@opentelemetry/resources": "^1.2.0",
    "@opentelemetry/sdk-node": "^0.28.0",
    "@opentelemetry/sdk-trace-base": "^1.2.0",
    "@opentelemetry/semantic-conventions": "^1.2.0",
    "dotenv": "^16.0.1",
    "fastify": "^3.29.0",
    "fastify-plugin": "^3.0.1",
    "http-terminator": "^3.2.0",
    "ioredis": "^5.0.4",
    "ms": "^2.1.3",
    "pino": "^7.11.0",
    "slonik": "^28.1.0",
    "umzug": "^3.1.1",
    "under-pressure": "^5.8.1",
    "zod": "^3.17.3",
    "zod-to-json-schema": "^3.17.0"
  },
  "overrides": {
    "esbuild": "^0.14.3"
  }
}
