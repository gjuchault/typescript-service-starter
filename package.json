{
	"name": "typescript-service-starter",
	"version": "0.0.0-development",
	"description": "Yet another typescript service starter template",
	"author": {
		"name": "Gabriel Juchault",
		"email": "gabriel.juchault@gmail.com"
	},
	"type": "module",
	"exports": "./build/index.js",
	"types": "./build/index.d.ts",
	"license": "MIT",
	"engines": {
		"node": "^23.5.0",
		"npm": "^10.9.2"
	},
	"volta": {
		"node": "23.5.0",
		"npm": "10.9.2"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"start:prod": "node --disable-warning=ExperimentalWarning --env-file=.env --env-file-if-exists=.env.local --experimental-strip-types ./src/index.ts",
		"start:dev": "node --disable-warning=ExperimentalWarning --env-file=.env --env-file-if-exists=.env.local --env-file=.env.development --env-file-if-exists=.env.development.local --experimental-strip-types ./src/index.ts | pino-pretty",
		"generate-client:ts": "node --disable-warning=DeprecationWarning --disable-warning=ExperimentalWarning --env-file=.env --env-file-if-exists=.env.local --experimental-strip-types ./scripts/generate-client.ts ts",
		"generate-client:json": "node --disable-warning=DeprecationWarning --disable-warning=ExperimentalWarning --env-file=.env --env-file-if-exists=.env.local --experimental-strip-types ./scripts/generate-client.ts json",
		"generate-client:yaml": "node --disable-warning=DeprecationWarning --disable-warning=ExperimentalWarning --env-file=.env --env-file-if-exists=.env.local --experimental-strip-types ./scripts/generate-client.ts yaml",
		"clean": "rm -rf coverage",
		"type:check": "tsc --noEmit",
		"lint": "biome check . --write",
		"lint:check": "biome ci .",
		"test": "node --disable-warning=ExperimentalWarning --experimental-strip-types ./scripts/test.ts test",
		"test:watch": "node --disable-warning=ExperimentalWarning --experimental-strip-types ./scripts/test.ts test:watch",
		"test:coverage": "node --disable-warning=ExperimentalWarning --experimental-strip-types ./scripts/test.ts test:coverage",
		"spell:check": "cspell \"{README.md,CODE_OF_CONDUCT.md,CONTRIBUTING.md,.github/*.md,src/**/*.ts}\"",
		"cz": "cz",
		"semantic-release": "semantic-release"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@ryansonshine/commitizen": "^4.2.8",
		"@ryansonshine/cz-conventional-changelog": "^3.3.4",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/commit-analyzer": "^13.0.0",
		"@semantic-release/github": "^11.0.1",
		"@semantic-release/npm": "^12.0.1",
		"@semantic-release/release-notes-generator": "^14.0.2",
		"@types/ms": "^0.7.34",
		"@types/node": "^22.10.2",
		"@types/prompts": "^2.4.9",
		"@types/yamljs": "^0.2.34",
		"c8": "^10.1.3",
		"cspell": "^8.17.1",
		"is-main": "^0.4.0",
		"openapi-types": "^12.1.3",
		"openapi-typescript": "^7.4.4",
		"pino-pretty": "^13.0.0",
		"rimraf": "^6.0.1",
		"semantic-release": "^24.2.0",
		"typescript": "^5.7.2"
	},
	"dependencies": {
		"@fastify/accepts-serializer": "^6.0.2",
		"@fastify/circuit-breaker": "^4.0.1",
		"@fastify/cors": "^10.0.1",
		"@fastify/formbody": "^8.0.1",
		"@fastify/helmet": "^13.0.0",
		"@fastify/multipart": "^9.0.1",
		"@fastify/rate-limit": "^10.2.1",
		"@fastify/secure-session": "^8.1.0",
		"@fastify/swagger": "^9.4.0",
		"@fastify/swagger-ui": "^5.2.0",
		"@fastify/under-pressure": "^9.0.2",
		"exit-hook": "^4.0.0",
		"fastify": "^5.2.0",
		"fastify-type-provider-zod": "^4.0.2",
		"http-terminator": "^3.2.0",
		"ms": "^2.1.3",
		"pino": "^9.6.0",
		"yamljs": "^0.3.0",
		"zod": "^3.24.1"
	}
}
