{
  "name": "@gjuchault/typescript-service-starter",
  "version": "0.0.0-development",
  "description": "Yet another typescript service starter template",
  "keywords": [
    "typescript",
    "service",
    "starter",
    "template"
  ],
  "homepage": "https://github.com/gjuchault/typescript-service-starter",
  "bugs": "https://github.com/gjuchault/typescript-service-starter/issues",
  "author": "Gabriel Juchault <gabriel.juchault@gmail.com>",
  "repository": "gjuchault/typescript-service-starter",
  "type": "module",
  "exports": "./build/index.js",
  "types": "./build/src/index.d.ts",
  "license": "MIT",
  "engines": {
    "node": "^18.17.0",
    "npm": "^9.5.1"
  },
  "volta": {
    "node": "18.17.0",
    "npm": "9.5.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "setup": "tsx ./scripts/setup.ts",
    "build": "npm run clean && npm run type:dts && npm run build:main",
    "build:main": "tsx ./scripts/build.ts",
    "start": "node ./build/index.js",
    "start:debug": "node --inspect ./build/index.js",
    "dev": "tsx ./scripts/dev.ts | pino-pretty",
    "clean": "tsx ./scripts/clean.ts",
    "migrate:up": "tsx ./scripts/migrate.ts up",
    "migrate:create": "tsx ./scripts/migrate.ts create",
    "type:dts": "tsc --emitDeclarationOnly --project tsconfig.build.json",
    "type:check": "tsc --noEmit",
    "format": "prettier \"src/**/*.ts\" --write",
    "format:check": "prettier \"src/**/*.ts\" --check",
    "lint": "eslint src --ext .ts --fix",
    "lint:check": "eslint src --ext .ts",
    "test": "PORT=1987 LOG_LEVEL=error vitest run",
    "test:watch": "PORT=1987 LOG_LEVEL=error vitest watch",
    "test:coverage": "PORT=1987 LOG_LEVEL=error vitest run --coverage",
    "test:setup": "tsx ./scripts/test-setup.ts",
    "test:healthcheck": "tsx ./scripts/test-healthcheck.ts",
    "spell:check": "cspell \"{README.md,CODE_OF_CONDUCT.md,CONTRIBUTING.md,.github/*.md,src/**/*.ts}\"",
    "cz": "cz",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@ryansonshine/commitizen": "^4.2.8",
    "@ryansonshine/cz-conventional-changelog": "^3.3.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^10.0.1",
    "@semantic-release/github": "^9.0.4",
    "@semantic-release/npm": "^10.0.4",
    "@semantic-release/release-notes-generator": "^11.0.4",
    "@types/eslint": "^8.44.2",
    "@types/ms": "^0.7.31",
    "@types/node": "^18.16.3",
    "@types/prompts": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "c8": "^8.0.1",
    "cspell": "^7.0.0",
    "esbuild": "^0.19.2",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "get-port": "^7.0.0",
    "launch-editor": "^2.6.0",
    "nyc": "^15.1.0",
    "pino-pretty": "^10.2.0",
    "prettier": "^3.0.1",
    "prompts": "^2.4.2",
    "semantic-release": "^21.0.7",
    "slugify": "^1.6.6",
    "source-map-support": "^0.5.21",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "vitest": "^0.34.1"
  },
  "dependencies": {
    "@autotelic/fastify-opentelemetry": "^0.17.1",
    "@fastify/circuit-breaker": "^3.2.0",
    "@fastify/cookie": "^9.0.4",
    "@fastify/cors": "^8.3.0",
    "@fastify/etag": "^4.2.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/helmet": "^11.0.0",
    "@fastify/multipart": "^7.7.3",
    "@fastify/rate-limit": "^8.0.3",
    "@fastify/under-pressure": "^8.3.0",
    "@gjuchault/typescript-service-sdk": "^3.5.0",
    "@opentelemetry/api": "^1.4.1",
    "@opentelemetry/api-metrics": "^0.33.0",
    "@opentelemetry/core": "^1.15.2",
    "@opentelemetry/exporter-prometheus": "^0.41.2",
    "@opentelemetry/resources": "^1.15.2",
    "@opentelemetry/sdk-node": "^0.41.2",
    "@opentelemetry/sdk-trace-base": "^1.15.2",
    "@opentelemetry/semantic-conventions": "^1.15.2",
    "@ts-rest/core": "^3.28.0",
    "@ts-rest/fastify": "^3.28.0",
    "@ts-rest/open-api": "^3.28.0",
    "bullmq": "^4.7.2",
    "dotenv": "^16.3.1",
    "fastify": "^4.21.0",
    "fastify-plugin": "^4.5.1",
    "http-terminator": "^3.2.0",
    "ioredis": "^5.3.2",
    "ms": "^2.1.3",
    "neverthrow": "^6.0.0",
    "pino": "^8.15.0",
    "slonik": "^34.1.0",
    "ts-pattern": "^5.0.5",
    "umzug": "^3.3.1",
    "zod": "^3.21.4"
  }
}
