{
	"name": "@gjuchault/typescript-service-starter",
	"version": "0.0.0-development",
	"description": "Yet another typescript service starter template",
	"keywords": [
		"typescript",
		"service",
		"starter",
		"template"
	],
	"homepage": "https://github.com/gjuchault/typescript-service-starter",
	"bugs": "https://github.com/gjuchault/typescript-service-starter/issues",
	"repository": "gjuchault/typescript-service-starter",
	"author": {
		"name": "Gabriel Juchault",
		"email": "gabriel.juchault@gmail.com"
	},
	"type": "module",
	"exports": "./client/schema.d.ts",
	"types": "./client/schema.d.ts",
	"license": "MIT",
	"engines": {
		"node": "^25.2.1",
		"npm": "^11.6.2"
	},
	"volta": {
		"node": "25.2.1",
		"npm": "11.6.2"
	},
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"client"
	],
	"scripts": {
		"start": "node --env-file=.env --env-file-if-exists=.env.local ./build/index.js",
		"dev": "node --env-file=.env --env-file-if-exists=.env.local --watch ./src/index.ts | pino-pretty",
		"dev:worker": "node --env-file=.env --env-file-if-exists=.env.local --watch ./src/worker.ts | pino-pretty",
		"worker": "node --env-file=.env --env-file-if-exists=.env.local ./build/worker.js",
		"bundle": "node ./scripts/bundle.ts",
		"generate-client": "node --env-file=.env --env-file-if-exists=.env.local ./scripts/generate-client.ts",
		"generate-secret": "node ./scripts/generate-secret.ts",
		"migrate:up": "node --env-file=.env --env-file-if-exists=.env.local ./scripts/migrate.ts up",
		"migrate:down": "node --env-file=.env --env-file-if-exists=.env.local ./scripts/migrate.ts down",
		"migrate:create": "node --env-file=.env --env-file-if-exists=.env.local ./scripts/migrate.ts create",
		"type:check": "tsc --noEmit",
		"lint": "biome check . --write",
		"lint:check": "biome ci .",
		"test": "node --env-file=.env --env-file-if-exists=.env.local --test-global-setup src/test-helpers/db-initial-setup.ts --test src/**/*.test.ts",
		"test:watch": "node --env-file=.env --env-file-if-exists=.env.local --test-global-setup src/test-helpers/db-initial-setup.ts --test --watch src/**/*.test.ts",
		"test:coverage": "c8 -r html node --env-file=.env --env-file-if-exists=.env.local --test-global-setup src/test-helpers/db-initial-setup.ts --test --experimental-test-coverage src/**/*.test.ts",
		"spell:check": "cspell \"{README.md,CODE_OF_CONDUCT.md,CONTRIBUTING.md,.github/*.md,src/**/*.ts}\"",
		"cz": "cz",
		"semantic-release": "semantic-release",
		"setup": "node ./scripts/setup.ts",
		"test:setup": "node ./scripts/test-setup.ts"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@ryansonshine/commitizen": "^4.2.8",
		"@ryansonshine/cz-conventional-changelog": "^3.3.4",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/commit-analyzer": "^13.0.1",
		"@semantic-release/github": "^12.0.2",
		"@semantic-release/npm": "^13.1.3",
		"@semantic-release/release-notes-generator": "^14.1.0",
		"@types/ms": "^2.1.0",
		"@types/node": "^25.0.5",
		"@types/prompts": "^2.4.9",
		"@types/yamljs": "^0.2.34",
		"c8": "^10.1.3",
		"cspell": "^9.4.0",
		"esbuild": "^0.27.2",
		"is-main": "^0.4.0",
		"launch-editor": "^2.12.0",
		"openapi-types": "^12.1.3",
		"openapi-typescript": "^7.10.1",
		"pino-pretty": "^13.1.3",
		"prompts": "^2.4.2",
		"rimraf": "^6.1.2",
		"semantic-release": "^25.0.2",
		"slugify": "^1.6.6",
		"typescript": "^5.9.3",
		"why-is-node-running": "^3.2.2"
	},
	"dependencies": {
		"@autotelic/fastify-opentelemetry": "^0.23.0",
		"@fastify/accepts-serializer": "^6.0.3",
		"@fastify/circuit-breaker": "^4.0.2",
		"@fastify/cookie": "^11.0.2",
		"@fastify/cors": "^11.2.0",
		"@fastify/formbody": "^8.0.2",
		"@fastify/helmet": "^13.0.2",
		"@fastify/multipart": "^9.3.0",
		"@fastify/rate-limit": "^10.3.0",
		"@fastify/session": "^11.1.1",
		"@fastify/swagger": "^9.6.1",
		"@fastify/under-pressure": "^9.0.3",
		"@opentelemetry/api": "^1.9.0",
		"@opentelemetry/exporter-metrics-otlp-proto": "^0.209.0",
		"@opentelemetry/exporter-trace-otlp-proto": "^0.209.0",
		"@opentelemetry/resource-detector-container": "^0.8.0",
		"@opentelemetry/resources": "^2.3.0",
		"@opentelemetry/sdk-metrics": "^2.3.0",
		"@opentelemetry/sdk-node": "^0.209.0",
		"@opentelemetry/sdk-trace-node": "^2.3.0",
		"@opentelemetry/semantic-conventions": "^1.38.0",
		"@valkey/valkey-glide": "^2.2.5",
		"connect-valkey": "^1.0.1",
		"exit-hook": "^5.0.1",
		"fastify": "^5.6.2",
		"fastify-type-provider-zod": "^6.1.0",
		"http-terminator": "^3.2.0",
		"ms": "^2.1.3",
		"pg-boss": "^12.5.4",
		"pino": "^10.1.1",
		"slonik": "^48.8.7",
		"temporal-polyfill": "^0.3.0",
		"ts-flowgen": "^2.2.1",
		"umzug": "^3.8.2",
		"yamljs": "^0.3.0",
		"zod": "^4.3.5"
	}
}
