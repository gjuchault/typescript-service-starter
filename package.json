{
  "name": "@gjuchault/typescript-service-starter",
  "version": "0.0.0-development",
  "description": "Yet another typescript service starter template",
  "keywords": [
    "typescript",
    "service",
    "starter",
    "template"
  ],
  "homepage": "https://github.com/gjuchault/typescript-service-starter",
  "bugs": "https://github.com/gjuchault/typescript-service-starter/issues",
  "author": "Gabriel Juchault <gabriel.juchault@gmail.com>",
  "repository": "gjuchault/typescript-service-starter",
  "type": "module",
  "exports": "./build/index.js",
  "types": "./build/src/index.d.ts",
  "license": "MIT",
  "engines": {
    "node": "^21.1.0",
    "npm": "^10.2.0"
  },
  "volta": {
    "node": "21.1.0",
    "npm": "10.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "setup": "tsx ./scripts/setup.ts",
    "build": "npm run clean && npm run type:dts && npm run build:main",
    "build:main": "tsx ./scripts/build.ts",
    "start": "node ./build/index.js",
    "start:debug": "node --inspect ./build/index.js",
    "dev": "tsx ./scripts/dev.ts | pino-pretty",
    "clean": "tsx ./scripts/clean.ts",
    "migrate:up": "tsx ./scripts/migrate.ts up",
    "migrate:create": "tsx ./scripts/migrate.ts create",
    "type:dts": "tsc --emitDeclarationOnly --project tsconfig.build.json",
    "type:check": "tsc --noEmit",
    "format": "prettier \"src/**/*.ts\" --write",
    "format:check": "prettier \"src/**/*.ts\" --check",
    "lint": "eslint src --ext .ts --fix",
    "lint:check": "eslint src --ext .ts",
    "test": "PORT=1987 LOG_LEVEL=error tsx --test \"src/**/*.test.ts\"",
    "test:watch": "PORT=1987 LOG_LEVEL=error tsx --watch --test \"src/**/*.test.ts\"",
    "test:coverage": "npm run clean && PORT=1987 LOG_LEVEL=error NODE_V8_COVERAGE=./coverage c8 -r html tsx --experimental-test-coverage --test \"src/**/*.test.ts\"",
    "test:setup": "tsx ./scripts/test-setup.ts",
    "test:healthcheck": "tsx ./scripts/test-healthcheck.ts",
    "spell:check": "cspell \"{README.md,CODE_OF_CONDUCT.md,CONTRIBUTING.md,.github/*.md,src/**/*.ts}\"",
    "cz": "cz",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@ryansonshine/commitizen": "^4.2.8",
    "@ryansonshine/cz-conventional-changelog": "^3.3.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/github": "^9.2.1",
    "@semantic-release/npm": "^11.0.1",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@types/eslint": "^8.44.7",
    "@types/ms": "^0.7.33",
    "@types/node": "^20.9.0",
    "@types/prompts": "^2.4.8",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "c8": "^8.0.1",
    "cspell": "^7.3.8",
    "esbuild": "^0.19.5",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "get-port": "^7.0.0",
    "launch-editor": "^2.6.1",
    "nyc": "^15.1.0",
    "pino-pretty": "^10.2.3",
    "prettier": "^3.0.3",
    "prompts": "^2.4.2",
    "semantic-release": "^22.0.7",
    "slugify": "^1.6.6",
    "source-map-support": "^0.5.21",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@fastify/circuit-breaker": "^3.2.0",
    "@fastify/cookie": "^9.1.0",
    "@fastify/cors": "^8.4.1",
    "@fastify/etag": "^4.2.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.0.0",
    "@fastify/rate-limit": "^8.0.3",
    "@fastify/under-pressure": "^8.3.0",
    "@gjuchault/typescript-service-sdk": "^4.5.0",
    "@opentelemetry/api": "^1.6.0",
    "@opentelemetry/api-metrics": "^0.33.0",
    "@opentelemetry/core": "^1.17.1",
    "@opentelemetry/exporter-prometheus": "^0.44.0",
    "@opentelemetry/resources": "^1.18.0",
    "@opentelemetry/sdk-node": "^0.44.0",
    "@opentelemetry/sdk-trace-base": "^1.18.0",
    "@opentelemetry/semantic-conventions": "^1.17.1",
    "@ts-rest/core": "^3.30.4",
    "@ts-rest/fastify": "^3.30.4",
    "@ts-rest/open-api": "^3.30.4",
    "bullmq": "^4.13.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3",
    "fastify-plugin": "^4.5.1",
    "http-terminator": "^3.2.0",
    "ioredis": "^5.3.2",
    "ms": "^2.1.3",
    "neverthrow": "^6.1.0",
    "pino": "^8.16.1",
    "slonik": "^37.2.0",
    "ts-pattern": "^5.0.5",
    "umzug": "^3.4.0",
    "zod": "^3.22.4"
  }
}
